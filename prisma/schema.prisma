

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Project{
  id  String @id @default(uuid())
  name String
  description String
  especificDetails String
  photos String[]
  @@map("projcts")
}
model User{
  id String @id @default(uuid())
  name String
  password String
}